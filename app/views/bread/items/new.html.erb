<h1>New Item</h1>

<%= form_for @item, :url => bread_items_path do |f| %>
  <%= render(:partial => 'form', :locals => { :f => f, :item => @item }) %>
<% end %>

<div class="submit_row">
  <%= link_to_function "Create", "$('form').last().submit();" %>
  <%= back_link :url => bread_items_path %>
</div>