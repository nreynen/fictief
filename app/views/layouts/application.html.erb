    <%= javascript_include_tag "application.js", "jquery.backstretch.min.js", "general/tabination.js" %>
    
    <%= yield :head %>
    
    <%= @js_mapping if @js_mapping %>
  </head>
  <body>
    <div id="main_overlay">
