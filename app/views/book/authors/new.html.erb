<h1>New Author</h1>

<%= form_for @author, :url => book_authors_path do |f| %>
  <%= render(:partial => 'form', :locals => { :f => f, :author => @author }) %>
<% end %>

<div class="submit_row">
  <%= link_to_function "Create", "$('form').last().submit();" %>
  <%= back_link :url => book_authors_path %>
</div>