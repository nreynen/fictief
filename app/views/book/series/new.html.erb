<h1>New Serie</h1>

<%= form_for @serie, :url => book_series_index_path do |f| %>
  <%= render(:partial => 'form', :locals => { :f => f, :serie => @serie }) %>
<% end %>

<div class="submit_row">
  <%= link_to_function "Create", "$('form').last().submit();" %>
  <%= back_link :url => book_series_index_path %>
</div>