<div>
  <h1>New publisher</h1>
  <table class="showtable">
    <% form_for @publisher, :url => book_publishers_path do |f| %>
      <%= render(:partial => 'form', :locals => { :f => f, :publisher => @publisher }) %>
      <tr>
        <td colspan="2"><%= f.submit "Create" %></td>
      </tr>
    <% end %>
    <tr class="button_row">
      <td colspan="2"><%= link_to "Back", book_publishers_path %></td>
    </tr>
  </table>
</div>