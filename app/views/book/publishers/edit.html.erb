<div>
  <h1>Editing publisher</h1>
  <table class="showtable">
    <% form_for @publisher, :url => publisher_path(@publisher) do |f| %>
      <%= render(:partial => 'form', :locals => { :f => f, :publisher => @publisher }) %>
      <tr>
        <td colspan="2"><%= f.submit "Update" %></td>
      </tr>
    <% end %>
    <tr class="button_row">
      <td colspan="2"><%= link_to "Show", book_publisher_path(@publisher) %> | <%= link_to "Back", book_publishers_path %></td>
    </tr>
  </table>
</div>